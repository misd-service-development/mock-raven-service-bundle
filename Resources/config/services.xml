<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="misd_mock_raven_service.routing.raven_loader.class">Misd\MockRavenServiceBundle\Routing\RavenLoader</parameter>
    </parameters>

    <services>

        <service id="misd_mock_raven_service.routing.raven_loader"
                 class="%misd_mock_raven_service.routing.raven_loader.class%"
                 public="false">
            <tag name="routing.loader"/>
        </service>

    </services>

</container>
